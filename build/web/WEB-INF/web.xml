<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>connectThem.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contactServlet</servlet-name>
        <servlet-class>connectThem.contactServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eQuestion</servlet-name>
        <servlet-class>connectThem.eQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mQuestion</servlet-name>
        <servlet-class>connectThem.mQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cQuestion</servlet-name>
        <servlet-class>connectThem.cQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>secQu</servlet-name>
        <servlet-class>connectThem.secQu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExcelDatabaseExport</servlet-name>
        <servlet-class>connectThem.ExcelDatabaseExport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>signUp</servlet-name>
        <servlet-class>connectThem.signServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signUp</servlet-name>
        <url-pattern>/signUp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>connectThem.logoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contactServlet</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eQuestion</servlet-name>
        <url-pattern>/eQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mQuestion</servlet-name>
        <url-pattern>/mQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cQuestion</servlet-name>
        <url-pattern>/cQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>secQu</servlet-name>
        <url-pattern>/secQu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExcelDatabaseExport</servlet-name>
        <url-pattern>/ExcelDatabaseExport</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
